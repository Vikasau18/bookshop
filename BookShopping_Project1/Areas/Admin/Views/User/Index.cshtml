
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-9">
        <h2 class="h1 fw-bold mb-5 mx-1 ">User List</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-info" asp-page="/Account/Register">
            <i class="fas fa-plus"></i>&nbsp;
            Create New User
        </a>
    </div>
</div>
<div class="border">
    <table id="tblData" class="table table-bordered table-striped">
        <thead>
            <tr class="table-secondary">
                <th>Name</th>
                <th>Email</th>

                <th>Phone Number</th>

                <th>Company</th>
                <th>Role</th>

                <th></th>

            </tr>
        </thead>
    </table>
</div>

@section scripts
{ 
<script src="~/js/user.js"></script>
}
